# OpenRouteService API ключ для построения маршрутов
VITE_ORS_API_KEY=your_api_key_here

# Базовый URL API бэкенда
VITE_API_BASE_URL=https://your-backend.com/api

# Секретный ключ для авторизации (используется в заголовке Authorization Bearer при запросе авторизации)
VITE_AUTH_SECRET_KEY=your_secret_key_here
